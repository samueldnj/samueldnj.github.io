	Note = {3rd \{GLOBEC\} OSM: From ecosystem function to ecosystem prediction},
	Number = {1{\^a}€``4},
	Pages = {338 - 346},
	Title = {Climate change, uncertainty, and resilient fisheries: Institutional responses through integrative science},
